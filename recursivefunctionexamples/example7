long f(long n)//n. fibonacci sayisini bulan fonksiyon
{
	if (n == 0 || n == 1)
	{
		return n;
	}
	else
	{
		return f(n - 1) + f(n - 2);
	}
}
int main(){
	long i, n;
	printf("How many fibonacci numbers?:");
	scanf("%d", &n);
	for (i = 1; i <= n; i++)
	{
	printf("Number %d: %ld\n", i, f(i));
	}
	return 0;
	}
